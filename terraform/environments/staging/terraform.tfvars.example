# General
# aws_region          = "ap-northeast-2"
# environment         = "staging"
# name_prefix         = "urr-staging"

# VPC
# vpc_cidr            = "10.1.0.0/16"

# EKS
# eks_cluster_version                      = "1.31"
# eks_cluster_endpoint_public_access       = true
# eks_cluster_endpoint_public_access_cidrs = ["0.0.0.0/0"]
# kms_key_arn                              = null
# eks_node_desired_size                    = 2
# eks_node_min_size                        = 1
# eks_node_max_size                        = 3
# eks_node_instance_types                  = ["t3.small"]
# eks_node_capacity_type                   = "SPOT"

# RDS
# rds_engine_version    = "16.4"
# rds_instance_class    = "db.t3.medium"
# rds_allocated_storage = 20

# MSK (Kafka)
# msk_broker_instance_type   = "kafka.t3.small"
# msk_broker_ebs_volume_size = 20

# ElastiCache
# elasticache_node_type = "cache.t4g.small"

# Domain & TLS
# domain_aliases       = []
# certificate_arn      = ""
# cors_allowed_origins = []

# Secrets (required, no defaults)
# internal_api_token = ""

# Monitoring
# sns_topic_arn = ""
